{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=5184000, must-revalidate"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; style-src 'self' 'unsafe-inline' fonts.googleapis.com vercel.live; img-src * blob: data: https:; media-src 'none'; connect-src *; font-src 'self' fonts.gstatic.com use.typekit.net data:; script-src 'self' us-assets.i.posthog.com insights.hotay.dev vercel.live 'sha256-qS5hf0AWdrLvX2VxhrWnUujUwPwmVlnqK2YKCCTe3jA=';"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://*.arcanapixel.com"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:html|js))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1209600, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:png|jpg|jpeg|gif|svg|ico|woff|woff2|css))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=5184000, must-revalidate"
        }
      ]
    }
  ]
}